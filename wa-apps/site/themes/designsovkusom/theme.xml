<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE theme PUBLIC "wa-app-theme" "http://www.webasyst.com/wa-content/xml/wa-app-theme.dtd">
<theme id="designsovkusom" system="0" vendor="988467" author="Designsovkusom" app="site" version="1.0.3.1">
  <name locale="en_US">Designsovkusom</name>
  <name locale="ru_RU">Дизайн со вкусом</name>
  <files>
    <file path="index.html">
      <description locale="en_US">Common layout</description>
      <description locale="ru_RU">Основной макет</description>
    </file>
    <file path="default.css">
      <description locale="en_US">Core CSS file</description>
      <description locale="ru_RU">Основной файл CSS-стилей</description>
    </file>
    <file path="mobile.css">
      <description locale="en_US">Additional CSS for multi-touch mobile devices</description>
      <description locale="ru_RU">Дополнительный CSS для мобильных устройств</description>
    </file>
    <file path="page.html">
      <description locale="en_US">Page content</description>
      <description locale="ru_RU">Содержимое страницы</description>
    </file>
    <file path="sidebar.html">
      <description locale="en_US">Page navigation sidebar</description>
      <description locale="ru_RU">Колонка с навигацией по страницам</description>
    </file>
    <file path="login.html">
		<description locale="en_US">Login page</description>
		<description locale="ru_RU">Страница входа для пользователей</description>
	</file>
    <file path="signup.html">
        <description locale="en_US">Sign up page</description>
        <description locale="ru_RU">Страница регистрации пользователей</description>
    </file>
    <file path="forgotpassword.html">
        <description locale="en_US">Forgot password page</description>
        <description locale="ru_RU">Страница сброса пароля</description>
    </file>
    <file path="error.html">
      <description locale="en_US">404 and other errors</description>
      <description locale="ru_RU">404 и другие ошибки</description>
    </file> 
  <file path="designsovkusom.css" custom="1"/><file path="designsovkusom.js" custom="1"/><file path="dsv.logo.html" custom="1"/><file path="dsv.contact.html" custom="1"/><file path="dsv.social.html" custom="1"/><file path="dsv.user.css" custom="1"/><file path="dsv.address.html" custom="1"/><file path="theme.color.css" custom="1"/></files>
  <settings>
  	<setting var="logo" control_type="image">
        <value/>
        <filename>img/logo.*</filename>
        <name locale="en_US">Company Logo</name>
        <name locale="ru_RU">Логотип компании</name>
    </setting>
  	<setting var="logo_slogan" control_type="text">
  		<value>Современный интернет-магазин</value>
  		<name locale="en_US">Text under logo</name>
  		<name locale="ru_RU">Подпись под логотипом</name>
  	</setting>
  	<setting var="vk" control_type="text">
  		<value>http://www.vk.com/</value>
  		<name locale="en_US">vkontakte community link</name>
  		<name locale="ru_RU">ссылка на сообщество вконтакте</name>
  	</setting>
  	<setting var="ok" control_type="text">
  		<value>http://www.odnoklassniki.ru/</value>
  		<name locale="en_US">odnoklassniki community link</name>
  		<name locale="ru_RU">ссылка на сообщество одноклассники</name>
  	</setting>
  	<setting var="tt" control_type="text">
  		<value>https://twitter.com/</value>
  		<name locale="en_US">twitter page</name>
  		<name locale="ru_RU">ссылка на twitter</name>
  	</setting>
  	<setting var="fb" control_type="text">
  		<value>https://www.facebook.com/</value>
  		<name locale="en_US">facebook community page</name>
  		<name locale="ru_RU">ссылка на сообщество facebook</name>
  	</setting>
  	<setting var="gg" control_type="text">
  		<value>https://plus.google.com/</value>
  		<name locale="en_US">google plus page</name>
  		<name locale="ru_RU">ссылка на google plus</name>
  	</setting>
  	<setting var="address" control_type="text">
  		<value>Москва, ул. Достоевского, 38</value>
  		<name locale="en_US">address in footer</name>
  		<name locale="ru_RU">Адрес внизу сайта</name>
  	</setting>
  	<setting var="news" control_type="checkbox">
  		<value>1</value>
  		<name locale="en_US">Show news in site</name>
  		<name locale="ru_RU">Отображать новости в сайт</name>
  	</setting>
  	<setting var="photos" control_type="checkbox">
  		<value>1</value>
  		<name locale="en_US">Show last photos in site</name>
  		<name locale="ru_RU">Отображать последние фотографии в сайт</name>
  	</setting>
  	<setting var="jivosite" control_type="checkbox">
  		<value>0</value>
  		<name locale="en_US">Jivosite on / off</name>
  		<name locale="ru_RU">Отображать консультант Jivosite</name>
  	</setting>
  	<setting var="widget_id" control_type="text">
  		<value></value>
  		<name locale="en_US">Jivosite widget id</name>
  		<name locale="ru_RU">Jivosite widget id</name>
  	</setting>
	 <setting var="color_scheme" control_type="image_select">
        <value>img/themesettings/blue.png</value>
        <name locale="ru_RU">Цветовая гамма</name>
        <name locale="en_US">Color scheme</name>
        <options>
            <option value="img/themesettings/blue.png"/>
            <option value="img/themesettings/blue2.png"/>
            <option value="img/themesettings/blue3.png"/>
            <option value="img/themesettings/darkblue.png"/>
            <option value="img/themesettings/green.png"/>
            <option value="img/themesettings/bermuda.png"/>
            <option value="img/themesettings/sanmarino.png"/>
            <option value="img/themesettings/orange.png"/>
            <option value="img/themesettings/red.png"/>
            <option value="img/themesettings/purple.png"/>
            <option value="img/themesettings/brown.png"/>
            <option value="img/themesettings/sun.png"/>
            <option value="img/themesettings/darkpink.png"/>
            <option value="img/themesettings/bluepurple.png"/>
        </options>
    </setting>
  	<setting var="my_color" control_type="color">
  		<value/>
  		<name locale="en_US">Own color</name>
  		<name locale="ru_RU">Свой цвет</name>
  	</setting>
    <setting var="rub" control_type="select">
        <value>text</value>
        <name locale="ru_RU">Написание рубля</name>
        <name locale="en_US">Ruble symbol</name>
        <options>
            <option value="text">
                <name locale="ru_RU">руб.</name>
                <name locale="en_US">руб.</name>
            </option>
            <option value="symbol">
                <name locale="ru_RU">Р</name>
                <name locale="en_US">Р</name>
            </option>
        </options>
    </setting>
  </settings>
</theme>
